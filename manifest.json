{
  "manifest_version": 2,

  "name": "Xmpp Chat client",
  "description": "A chrome extension for XMPP chat.",
  "version": "1.0",

  "permissions": [
    "https://nothing.com/"
  ],
  
  "browser_action": {
    "default_icon": "resources/chat_icon.jpg",
    "default_popup": "main.html",
    "default_title": "Google chat"
  }
}